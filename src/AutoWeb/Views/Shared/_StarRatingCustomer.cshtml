@model MINI.Models.RatingModel
@{
    for (int i = 1; i <= Model.Stars; i++)
    {<img alt="" src="~/public/img/star-rating/star.png" />}
    if (Model.Stars < 5)
    {
        int left = 5 - Model.Stars;
        for (int i = 1; i <= left; i++)
        {<img alt="" src="~/public/img/star-rating/no-star.png" />}
    } @Html.Raw("&nbsp;") @Model.Value
}